{
  "ip.address": "127.0.0.1",
  "backend.version": 1,
  "backend.id": 1,
  "backend.http.port": 2491,
  "leader.http.port": 2496,
  "load.report.interval.secs": 60,
  "recorder.defunct.threshold.secs": 120,
  "slot.pool.capacity": 100,
  "backend.http.server": {
    "idle.timeout.secs": 120
  },
  "leader.http.server": {
    "idle.timeout.secs": 120
  },
  "http.client": {
    "connect.timeout.ms": 5000,
    "idle.timeout.secs": 120,
    "max.attempts": 3,
    "keepalive": true,
    "compression": true
  },
  "vertxOptions": {
    "blockedThreadCheckInterval": 1000000
  },
  "backendHttpOptions": {
    "config": {
      "verticle.count": 2,
      "report.load": true,
      "parser": {
        "recordingheader.max.bytes": 1024,
        "wse.max.bytes": 1048576
      }
    }
  },
  "curatorOptions": {
    "connection.url": "",
    "namespace": "fkprof",
    "connection.timeout.ms": 5000,
    "session.timeout.ms": 60000,
    "max.retries": 3
  },
  "leaderElectionOptions": {
    "worker": true,
    "config": {
      "aggregation.enabled": false,
      "leader.watching.path": "/backends",
      "leader.mutex.path": "/leader",
      "leader.sleep.ms": 60000,
      "kill.behavior": "KILL"
    }
  },
  "leaderHttpOptions": {
    "backend.association.path": "/association",
    "load.miss.tolerance": 1
  },
  "daemonOptions": {
    "worker": true,
    "config": {
      "aggregation.window.duration.secs": 1800,
      "aggregation.window.end.tolerance.secs": 120,
      "policy.refresh.offset.secs": 300,
      "scheduling.buffer.secs": 30,
      "work.assignment.max.delay.secs": 120
    }
  },
  "storage": {
    "s3": {
      "end.point": "http://127.0.0.1:13031",
      "access.key": "",
      "secret.key": ""
    },
    "thread.pool.size": 50,
    "base.dir": "profiles"
  },
  "fixedSizebufferPool" : {
    "max.total": 20,
    "max.idle": 20,
    "buffer.size": 10000000
  }
}